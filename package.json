{ "name": "noflo-packets"
, "description": "Packet Utilities for NoFlo"
, "keywords":
  [ "noflo"
  , "packets"
  , "connections"
  , "utilities"
  ]
, "author": "Kenneth Kan <kenhkan@gmail.com>"
, "version": "0.0.1"
, "licenses":
  [ { "type": "MIT"
    , "url": "https://github.com/kenhkan/noflo-packets/blob/master/LICENSE.md"
    }
  ]
, "engines":
  { "node": ">=0.6.0"
  }
, "repository":
  { "type": "git"
  , "url": "git://github.com/kenhkan/noflo-packets"
  }
, "dependencies":
  { "noflo": "0.3.x"
  , "underscore": "1.4.x"
  , "underscore.string": "2.3.x"
  , "noflo-flow": "0.0.x"
  }
, "devDependencies":
  { "coffeelint": "*"
  , "coffee-script": "1.6.x"
  , "noflo-test": "https://github.com/kenhkan/noflo-test/tarball/develop"
  }
, "noflo":
  { "components":
    { "Arrayify": "./components/Arrayify.coffee"
    , "Counter": "./components/Counter.coffee"
    , "Defaults": "./components/Defaults.coffee"
    , "DoNotDisconnect": "./components/DoNotDisconnect.coffee"
    , "OnlyDisconnect": "./components/OnlyDisconnect.coffee"
    , "SplitPacket": "./components/SplitPacket.coffee"
    }
  , "graphs":
    { "MergeConnections": "./graphs/MergeConnections.fbp"
    }
  }
, "scripts":
  { "pretest": "./node_modules/.bin/coffeelint -r components"
  , "test": "./node_modules/.bin/noflo-test --spec test/*.coffee"
  }
}
